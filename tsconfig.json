{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES2022",
    "target": "es2022",
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*.astro"
      ],
      "@layouts/*": [
        "src/components/layouts/*.astro"
      ],
      "@constants/*": [
        "src/constants/*.ts"
      ],
      "@lib/*": [
        "src/lib/*.ts"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}